{
  "name": "srag/helpmecron",
  "description": "HelpMeCron",
  "version": "1.3.2",
  "extra": {
    "ilias_plugin": {
      "id": "srsucron",
      "name": "HelpMeCron",
      "ilias_min_version": "6.0",
      "ilias_max_version": "6.999",
      "slot": "Services/Cron/CronHook",
      "supports_cli_setup": true
    },
    "generate_plugin_readme_template": "SRAG_ILIAS_PLUGIN",
    "long_description_template": "src/LONG_DESCRIPTION.md"
  },
  "type": "project",
  "keywords": [
    "cron",
    "help",
    "ilias",
    "job",
    "plugin",
    "support"
  ],
  "homepage": "https://github.com/fluxfw/HelpMeCron",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "studer + raimann ag - Team Custom 1",
      "email": "support-custom1@studer-raimann.ch",
      "homepage": "https://studer-raimann.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://plugins.studer-raimann.ch/goto.php?target=uihk_srsu_PLHM"
  },
  "require": {
    "php": ">=7.2",
    "srag/generateplugininfoshelper": ">=0.1.0",
    "srag/librariesnamespacechanger": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\Plugins\\HelpMeCron\\": "src/"
    },
    "classmap": [
      "classes/"
    ],
    "files": [
      "../../../UIComponent/UserInterfaceHook/HelpMe/vendor/autoload.php"
    ]
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "pre-autoload-dump": [
      "srag\\LibrariesNamespaceChanger\\LibrariesNamespaceChanger::rewriteLibrariesNamespaces",
      "srag\\GeneratePluginInfosHelper\\HelpMeCron\\GeneratePluginPhpAndXml::generatePluginPhpAndXml",
      "srag\\GeneratePluginInfosHelper\\HelpMeCron\\GeneratePluginReadme::generatePluginReadme"
    ]
  }
}
